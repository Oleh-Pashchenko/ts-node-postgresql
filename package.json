    "express-async-errors": "^3.0.0",
    "express-flash": "0.0.2",
    "express-rate-limit": "^3.1.0",
    "express-session": "^1.15.6",
    "express-validation": "^1.0.2",
    "express-validator": "^4.3.0",
    "fbgraph": "^1.4.1",
    "geoip-lite": "^1.3.1",
    "iplocation": "^5.0.1",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.4",
    "lusca": "^1.5.2",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
